            <TextField fx:id="depTextField" editable="false" layoutX="102.0" layoutY="176.0" />
            <TextField fx:id="cprTextField" editable="false" layoutX="102.0" layoutY="208.0" />
            <Button fx:id="schemaButton" layoutX="24.0" layoutY="254.0" mnemonicParsing="false" onAction="#schemaButtonHandler" text="Aktivitetsskemaer" />
            <Button fx:id="backButton" layoutX="15.0" layoutY="350.0" mnemonicParsing="false" onAction="#backButtonHandler" prefHeight="25.0" prefWidth="55.0" text="Tilbage" />
