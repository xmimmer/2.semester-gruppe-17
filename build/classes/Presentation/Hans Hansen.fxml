<AnchorPane id="AnchorPane" fx:id="anchorPane1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Logic.HansHansenController">
   <children>
      <Pane fx:id="pane1" prefHeight="400.0" prefWidth="600.0">
         <children>
