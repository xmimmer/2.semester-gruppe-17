            </ImageView>
            <Label fx:id="ageLabel" layoutX="30.0" layoutY="140.0" text="Alder">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="depLabel" layoutX="30.0" layoutY="175.0" text="Afdeling">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="cprLabel" layoutX="30.0" layoutY="207.0" text="CPR-nr.">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="diaryLabel" layoutX="447.0" layoutY="153.0" text="Dagbog">
